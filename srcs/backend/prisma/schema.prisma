generator client {
  provider = "prisma-client-js"
  output     = "../src/generated/prisma"
  engineType    = "library"
}

datasource db {
  provider = "postgresql"
}

model User {
  id        Int      @id @default(autoincrement())
  email     String   @unique
  name      String?
  provider  String
  googleId  String?  @unique
  createdAt DateTime @default(now())
}

